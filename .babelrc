{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",[
    "prismjs",
    {
      "languages": ["javascript", "css", "markup"], //这是语言类型
      "plugins": ["line-numbers"],//使用了行号还可以添加其他插件，具体去官网看看吧
      "theme": "twilight",//主题！！
      "css": true
    }
  ]]
}
